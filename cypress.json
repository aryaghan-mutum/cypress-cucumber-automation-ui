{
    "baseUrl" : "https://google.com",
    "testFiles": "**/*.{feature,features}",
    "viewportWidth": 1200,
    "viewportHeight": 720,
    "defaultCommandTimeout": 1000,
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        }
    }
}

